{
  "name": "serengpty",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/serengpty",
  "projectType": "application",
  "dependencies": {
    "@enclaveid/ui-utils": "*"
  },
  "tags": [],
  "targets": {
    "prisma-deploy": {
      "executor": "@nx-tools/nx-prisma:deploy"
    },
    "prisma-format": {
      "executor": "@nx-tools/nx-prisma:format"
    },
    "prisma-generate": {
      "executor": "@nx-tools/nx-prisma:generate"
    },
    "prisma-migrate": {
      "executor": "@nx-tools/nx-prisma:migrate",
      "options": {
        "name": "migrate"
      }
    },
    "prisma-pull": {
      "executor": "@nx-tools/nx-prisma:pull"
    },
    "prisma-push": {
      "executor": "@nx-tools/nx-prisma:push"
    },
    "prisma-reset": {
      "executor": "@nx-tools/nx-prisma:reset",
      "options": {
        "force": true
      }
    },
    "prisma-resolve": {
      "executor": "@nx-tools/nx-prisma:resolve"
    },
    "prisma-post-migrate": {
      "executor": "@nx-tools/nx-prisma:seed",
      "options": {
        "script": "apps/serengpty/prisma/postMigrate.ts"
      }
    },
    "prisma-status": {
      "executor": "@nx-tools/nx-prisma:status"
    },
    "prisma-studio": {
      "executor": "@nx-tools/nx-prisma:studio"
    },
    "prisma-validate": {
      "executor": "@nx-tools/nx-prisma:validate"
    },
    "prisma-nuke": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/serengpty",
        "commands": [
          "rm -rf prisma/migrations",
          "yes | nx run serengpty:prisma-migrate",
          "cd ../.. && nx run serengpty:prisma-post-migrate"
        ],
        "parallel": false
      }
    }
  }
}
